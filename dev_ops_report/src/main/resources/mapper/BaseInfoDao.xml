<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aspire.devops.report.dao.BaseInfoDao">

	<select id="list" resultType="com.aspire.devops.report.model.ProjectInfo">
		select a._id as pid, Null as sub_pid,_name as pname, '' as sub_pname, 1 as type
			from zt_project a ORDER  by _name
	</select>

</mapper>